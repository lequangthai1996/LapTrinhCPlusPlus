    swap(A[j],A[j-1]);\u003Cbr \/\u003E                     }\u003Cbr \/\u003E             }\u003Cbr \/\u003E     }\u003Cbr \/\u003E     \u003Cbr \/\u003E}\u003Cbr \/\u003Evoid swap(int \u0026amp;x,int \u0026amp;y)\u003Cbr \/\u003E{\u003Cbr \/\u003E     tmp=x;\u003Cbr \/\u003E     x=y;\u003Cbr \/\u003E     y=tmp;\u003Cbr \/\u003E}\u003C\/pre\u003E\u003C\/div\u003E\u003Cspan style=\"background-color: #fffffc; color: #333333; font-family: \u0026quot;verdana\u0026quot; , \u0026quot;arial\u0026quot; , \u0026quot;tahoma\u0026quot; , \u0026quot;calibri\u0026quot; , \u0026quot;geneva\u0026quot; , sans-serif; font-size: 13px;\"\u003E4\/\u0026nbsp;\u003C\/span\u003E\u003Cb style=\"background-color: #fffffc; color: #333333; font-family: Verdana, Arial, Tahoma, Calibri, Geneva, sans-serif; font-size: 13px; margin: 0px; padding: 0px;\"\u003E\u003Cb style=\"margin: 0px; padding: 0px;\"\u003EQuick sort\u003C\/b\u003E\u003C\/b\u003E\u003Cbr \/\u003E\u003Cspan style=\"background-color: #fffffc; color: #333333; font-family: \u0026quot;verdana\u0026quot; , \u0026quot;arial\u0026quot; , \u0026quot;tahoma\u0026quot; , \u0026quot;calibri\u0026quot; , \u0026quot;geneva\u0026quot; , sans-serif; font-size: 13px;\"\u003Emình thích nhất cái này .Thuật toán này chia dãy thành 2 phần tách biệt nhau bởi 1 phần tử pivot , dãy 1 gồm các phần tử nhỏ hơn pivot , dãy 2 gồm các phần tử lớn hơn pivot ,với mục tiêu hoán đổi vị trí các phần tử nhỏ hơn pivot được chuyển sang bên trái , các phần tử lớn hơn sang bên phải,\u0026nbsp;\u003C\/span\u003E\u003Cbr \/\u003E\u003Cspan style=\"background-color: #fffffc; color: #333333; font-family: \u0026quot;verdana\u0026quot; , \u0026quot;arial\u0026quot; , \u0026quot;tahoma\u0026quot; , \u0026quot;calibri\u0026quot; , \u0026quot;geneva\u0026quot; , sans-serif; font-size: 13px;\"\u003Esau đó giải 2 bài toán nhỏ bằng đệ quy\u003C\/span\u003E\u003Cbr \/\u003E\u003Cdiv class=\"bbcode_container bbcode_container_code\" style=\"background-color: #fffffc; border: 1px solid rgb(204, 206, 219); color: #333333; font-family: Verdana, Arial, Tahoma, Calibri, Geneva, sans-serif; font-size: 13px; margin: 0px 0px 10px; padding: 0px;\"\u003E\u003Cbr \/\u003E\u003Cpre class=\"bbcode_code\" style=\"background: none rgb(255, 255, 255); direction: ltr; font-size: 12px; height: 336px; overflow: auto; padding: 6px;\"\u003Evoid swap(int \u0026amp;x,int \u0026amp;y)  \u003Cbr \/\u003E{\u003Cbr \/\u003E     tmp=x;\u003Cbr \/\u003E     x=y;\u003Cbr \/\u003E     y=tmp;\u003Cbr \/\u003E}\u003Cbr \/\u003E\u003Cbr \/\u003Evoid quick_sort(int A[],int first,int last) \u003Cbr \/\u003E{\u003Cbr \/\u003E       \u003Cbr \/\u003E              if(first\u0026gt;=last) return;\u003Cbr \/\u003E              i=first;\u003Cbr \/\u003E              j=last;\u003Cbr \/\u003E              pivot=A[(first+last)\/2];  \/\/ chon phan tu \" trung vi \" lam chot\u003Cbr \/\u003E              while(i\u0026lt;j)\u003Cbr \/\u003E                 {\u003Cbr \/\u003E                       \u003Cbr \/\u003E                       while((A[i]\u0026lt;pivot)\u0026amp;\u0026amp;(i\u0026lt;last))i++; \/\/ nua trai nho hon pivot\u003Cbr \/\u003E                       while((A[j]\u0026gt;pivot)\u0026amp;\u0026amp;(j\u0026gt;first))j--; \/\/ nua phai lon hon pivot\u003Cbr \/\u003E                       if(i\u0026lt;j)swap(A[i],A[j]);       \/\/ Co ban la i chay tu trai sang phai . Neu gap phan tu lon hon pivot thi tiep\u003Cbr \/\u003E                 }                                        \/\/ tuc chay  ( i++ ) còn nếu gặp phan tu lon hon pivot thi i dung lai den khi j \u003Cbr \/\u003E                                                          \/\/ cung dung lai (gap phan tu nho hon pivot) thi thuc hien trao doi vi tri\u003Cbr \/\u003E\u003Cbr \/\u003E                       quick_sort(A,first,i-1); \/\/ goi De quy de sap xep nua trai cua pivot\u003Cbr \/\u003E                       quick_sort(A,i+1,last);  \/\/ goi Dq quy de sap xep nua phai cua pivot\u003Cbr \/\u003E}\u003C\/pre\u003E\u